{
  "name": "video-player",
  "version": "1.0.0",
  "description": "A HTML5 video player",
  "main": "lib/index.js",
  "scripts": {
    "test": "karma start",
    "start": "parcel serve ./demo/index.html --open --no-cache",
    "rollup": "rollup -c",
    "scss": "node ./build-scss",
    "es": "tsc --module ESNext -d --outDir ./es",
    "lib": "tsc --module commonjs -d --outDir ./lib",
    "build": "node rmdir && npm run es && npm run lib && npm run rollup && npm run scss",
    "publish": "npm run build && npm run publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xroah/video-player.git"
  },
  "author": "X Roah",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xroah/video-player/issues"
  },
  "homepage": "https://github.com/xroah/video-player#readme",
  "keywords": [
    "html5",
    "video",
    "player"
  ],
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@types/jasmine": "^3.5.14",
    "babel-plugin-istanbul": "^6.0.0",
    "jasmine": "^3.6.1",
    "karma": "^5.2.3",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-typescript": "^5.2.0",
    "parcel": "^2.0.0-beta.1",
    "rollup": "^2.28.2",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.26.11",
    "tslib": "^2.0.1",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.11.2"
  },
  "files": [
    "dist",
    "lib",
    "es"
  ]
}